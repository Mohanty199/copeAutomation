{
  "name": "copeautomation-wdio-cucumber",
  "version": "1.0.0",
  "description": "Cucubmer Wdio project for E2E testing of Web Application",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "demo":"npx wdio wdio.conf.ts --cucumberOpts.tagExpression='@demo'",
    "smoke":"npx wdio wdio.conf.ts --cucumberOpts.tagExpression='@smoke'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "wdio": "wdio run ./wdio.conf.ts",
    "start": "babel-node ./wdio.conf.ts"
  },
  "keywords": [
    "wdio",
    "cucumber",
    "e2e",
    "test"
  ],
  "author": "Deepak Mohanty",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/node": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@types/chai": "^4.3.5",
    "@wdio/allure-reporter": "^8.15.0",
    "@wdio/cli": "^8.15.0",
    "@wdio/cucumber-framework": "^8.15.0",
    "@wdio/local-runner": "^8.15.0",
    "@wdio/spec-reporter": "^8.15.0",
    "chai": "^4.3.7",
    "chromedriver": "^115.0.1",
    "geckodriver": "^4.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "wdio-chromedriver-service": "^8.1.1",
    "wdio-geckodriver-service": "^5.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ]
  },
  "dependencies": {
    "@wdio/types": "^8.15.0"
  }
}
